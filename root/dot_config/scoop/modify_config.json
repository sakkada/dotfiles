chezmoi:modify-template
chezmoi:template:line-endings=crlf
{{- $input := .chezmoi.stdin | fromJson -}}
{
  "last_update": "{{ index $input "last_update" | default "1970-01-01T00:00:00.000000+00:00" }}",
  "alias": {
    "cleanup-all": "scoop-cleanup-all"
  },
  "scoop_branch": "master",
  "scoop_repo": "https://github.com/ScoopInstaller/Scoop"
}
